{
  "name": "arena-core",
  "version": "1.14.1",
  "description": "Core browser client for ARENA",
  "author": "CONIX Center",
  "license": "BSD-3-Clause",
  "default": "dist/arena-core.js",
  "browserslist": [
    "last 2 firefox versions"
  ],
  "alias": {
    "three": "super-three"
  },
  "overrides": {
    "aframe": "git+https://github.com/arenaxr/aframe.git#db9222b",
    "super-three": "0.147.0",
    "three-pathfinding": "^1.1.0"
  },
  "dependencies": {
    "aframe": "git+https://github.com/arenaxr/aframe.git#db9222b",
    "aframe-blink-controls": "^0.4.3",
    "aframe-environment-component": "^1.3.1",
    "aframe-extras": "git+https://github.com/n5ro/aframe-extras#ebae62f",
    "aframe-particle-system-component": "^1.1.3",
    "aframe-spe-particles-component": "^1.0.4",
    "aframe-thickline-component": "^0.0.3",
    "buffer": "^6.0.3",
    "comlink": "^4.3.0",
    "core-js": "^3.20.3",
    "events": "^3.3.0",
    "he": "^1.2.0",
    "jquery": "^3.6.0",
    "linkifyjs": "^2.1.9",
    "mqtt-pattern": "^1.2.0",
    "paho-mqtt": "^1.1.0",
    "sweetalert2": "^10.16.6",
    "three-pathfinding": "^1.1.0",
    "uuidjs": "^4.2.11",
    "webxr-polyfill": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.17.0",
    "babel": "^6.23.0",
    "babel-loader": "^8.1.0",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.22.0",
    "eslint-config-google": "^0.14.0",
    "jsdoc": "^3.6.6",
    "jsdoc-to-markdown": "^7.0.1",
    "path-browserify": "^1.0.1",
    "raw-loader": "^4.0.1",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0"
  },
  "scripts": {
    "collect-static": "cp src/systems/armarker/apriltag-detector/apriltag*.* dist/",
    "prewatch": "npm run collect-static",
    "watch": "webpack --mode development --progress --watch",
    "prebuild": "npm run collect-static",
    "build": "webpack --config webpack.config.js --mode production; echo Built at `date`",
    "build-nominify": "webpack --config webpack.config.js --mode development; echo Built at `date`",
    "predocs": "echo '# A-Frame components (modules) added to support ARENA core functionality\n' > src/components/README.md; echo '# A-Frame systems (modules) added to support ARENA core functionality\n' > src/systems/README.md",
    "docs": "jsdoc2md src/components/*.js >> src/components/README.md; jsdoc2md src/systems/*.js >> src/systems/README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arenaxr/arena-web-core.git"
  },
  "bugs": {
    "url": "https://github.com/arenaxr/arena-web-core/issues"
  },
  "homepage": "https://github.com/arenaxr/arena-web-core#readme",
  "webpackExclude": [
    "apriltag.js",
    "apriltag_wasm.js",
    "apriltag_wasm.wasm"
  ]
}
